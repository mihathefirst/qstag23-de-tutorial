{
    // "python.defaultInterpreterPath": "/home/gitpod/.pyenv/shims/python",
    "python.terminal.activateEnvironment": false,
    "gitpod.experimental.portsView.enabled": false,
    "files.autoSave": "onFocusChange",
    "explorer.compactFolders": false,
    "git.autorefresh": true,
    "git.autofetchPeriod": 10,
    // "robotcode.robot.languages": ["de"],
    "robotcode.robot.pythonPath": [
        "./",
        "./live",
        "./live.Examples.Example2"
    ],
    "robotcode.robot.args": [
        "--loglevel",
        "TRACE:INFO"
        // "INFO"
    ],
    "robotcode.debug.outputMessages": false,
    "robotcode.debug.groupOutput": false,
    "robotcode.debug.attachPython": true,
    "robotcode.robocop.enabled": false,
    "redhat.telemetry.enabled": false,
    "workbench.colorTheme": "Default Dark+",
    "gitpod.openInStable.neverPrompt": true,
    "git.autofetch": true,
    "github.gitProtocol": "ssh",
    "files.trimTrailingWhitespace": true,
    "githubPullRequests.createOnPublishBranch": "never",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "files.exclude": {
        "**/.*": true,
        "LICENSE": true,
        "requirements.txt": true,
        "README.md": true,
		"**/live/*.*": true,
		"**/live/Examples/CustomCode": true,
		"**/live/Examples/Example_50": true,
		"**/live/Examples/Example1": true,
		"**/live/Examples/ExampleBDD": true,
		"**/live/Examples/ExampleEntry": true,
		"**/live/Examples/ExampleJS": true,
		"**/live/Examples/ExampleXML": true,
		"**/live/Examples/FOR_Loops": true,
		"**/live/Examples/IF_Statement": true,
		"**/live/Examples/Variables": true,
		"**/live/Examples/index.json": true,
		"**/live/Examples/Example2/test.robotx": true,
		"**/live/Examples/Example2/test.pyx": true,
		"**/live/Examples/Example2/config.json": true,
		"**/crypto_keys": true
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorCustomizations": {
        "[Default Light+]": {
            // "editor.background": "#0d0f14",
            "terminal.background": "#000000",

            // "sideBar.background": "#0d0f14",
            // "sideBar.border": "#3f7bcf",
            "list.hoverBackground": "#3f7bcf",
            "list.hoverForeground": "#ffffff",
            "list.activeSelectionForeground": "#ffffff",
            "list.activeSelectionIconForeground": "#ffffff",
            "menu.selectionForeground": "#ffffff",
            "tab.inactiveBackground": "#0d0f14",
            "tab.inactiveForeground": "#ffffff",
            "tab.border": "#3f7bcf",
            "gitDecoration.modifiedResourceForeground": "#008Aff",
            "gitDecoration.untrackedResourceForeground": "#aaaaaa",
            "activityBar.activeBackground": "#3f7bcf",
            "activityBar.background": "#0d0f14",
            "activityBar.inactiveForeground": "#ffffff",
            "activityBar.border": "#3f7bcf",
            "debugConsole.warningForeground": "#0d0f14",
            "debugConsole.sourceForeground": "#008Aff",
            "terminal.foreground": "#ffffff",
            "terminal.ansiGreen": "#00ff00",
            "terminal.ansiRed": "#ff6600"
        }
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "variable.function.keyword-call.inner.robotframework",
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "variable.function.keyword-call.robotframework",
                "settings": {
                    //"fontStyle": "bold"
                }
            },
            {
                "scope": "entity.name.function.testcase.name.robotframework",
                "settings": {
                    "fontStyle": "underline",
                    "foreground": "#3f7bcf"

                }
            },
            {
                "scope": "entity.name.function.keyword.name.robotframework",
                "settings": {
                    "fontStyle": "underline italic",
                    "foreground": "#3f7bcf"

                }
            },
            {
                "scope": "variable.other.readwrite.robotframework",
                "settings": {
                    //"fontStyle": "italic",
                }
            },
            {
                "scope": "keyword.control.import",
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "keyword.other.header",
                "settings": {
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "keyword.other.header.comment.robotframework",
                "settings": {
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "constant.character.escape",
                "settings": {
                    "foreground": "#6666ff",
                }
            }
        ]
    },
    "python.autoComplete.extraPaths": [

    ],
    "python.analysis.extraPaths": [],
}